<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <magendoo_productwebhook>
            <general>
                <enabled>0</enabled>
                <endpoint></endpoint>
                <use_queue>1</use_queue>
                <timeout>10</timeout>
                <allowed_attributes>sku,name,price,status,visibility,type_id,weight</allowed_attributes>
            </general>
        </magendoo_productwebhook>
    </default>
</config>
